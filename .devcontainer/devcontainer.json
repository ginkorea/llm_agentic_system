{
  "name": "Python 3 Development Environment",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  },
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "forwardPorts": [8080],
  "postCreateCommand": "pip install .",
  "remoteUser": "vscode"
}
